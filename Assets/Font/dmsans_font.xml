<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:font="@font/dmsans_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/dmsans_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/dmsans_bolditalic"
        android:fontStyle="italic"
        android:fontWeight="700"
        app:font="@font/dmsans_bolditalic"
        app:fontStyle="italic"
        app:fontWeight="700" />

    <font
        android:font="@font/dmsans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/dmsans_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/dmsans_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/dmsans_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <font
        android:font="@font/dmsans_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/dmsans_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <font
        android:font="@font/dmsans_mediumitalic"
        android:fontStyle="italic"
        android:fontWeight="500"
        app:font="@font/dmsans_mediumitalic"
        app:fontStyle="italic"
        app:fontWeight="500" />
</font-family>